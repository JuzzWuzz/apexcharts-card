{
  "name": "apexcharts-card",
  "version": "1.0.0",
  "description": "A Home Assistant Lovelace card using ApexCharts to render graphs",
  "main": "src/apexcharts-card.ts",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && npm run build:check && npm run lint && npm run rollup",
    "build:check": "ts-interface-builder src/types-config.ts",
    "lint": "eslint --fix src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "rollup": "rollup -c rollup.config.js",
    "start": "rollup -c rollup.config.js --watch"
  },
  "keywords": [
    "lovelace",
    "apexcharts",
    "graphs",
    "charts",
    "home-assistant"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/moment-duration-format": "^2.2.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "apexcharts": "^3.37.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "juzz-ha-helper": "file:../juzz-ha-helper",
    "prettier": "^2.8.4",
    "prettier-plugin-multiline-arrays": "^1.1.3",
    "rimraf": "^4.4.0",
    "rollup": "^3.19.1",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-interface-builder": "^0.3.3",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.6.0",
    "home-assistant-js-websocket": "^8.0.1",
    "lit": "^2.6.1",
    "moment": "^2.29.4",
    "tinycolor": "^0.0.1",
    "ts-interface-checker": "^1.0.2"
  }
}

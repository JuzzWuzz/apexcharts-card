{
  "name": "apexcharts-card-2",
  "version": "2.0.2",
  "description": "A Home Assistant Lovelace card using ApexCharts to render graphs",
  "main": "src/apexcharts-card.ts",
  "scripts": {
    "build": "rimraf dist && npm run build:check && npm run lint && npm run rollup",
    "build:check": "ts-interface-builder src/types-config.ts",
    "check": "npm run build:check && npm run format:check && npm run lint:check",
    "lint": "eslint --fix src/**/*.ts",
    "lint:check": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier src/**/*.ts",
    "rollup": "rollup -c rollup.config.js",
    "start": "rollup -c rollup.config.js --watch"
  },
  "keywords": [
    "lovelace",
    "apexcharts",
    "graphs",
    "charts",
    "home-assistant"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/moment-duration-format": "^2.2.3",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "apexcharts": "^3.37.1",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.4",
    "prettier-plugin-multiline-arrays": "^1.1.3",
    "rimraf": "^4.3.0",
    "rollup": "^2.79.1",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-interface-builder": "^0.3.3",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.4.0",
    "@material/mwc-ripple": "^0.25.3",
    "array-flat-polyfill": "^1.0.1",
    "custom-card-helpers": "^1.9.0",
    "home-assistant-js-websocket": "^7.0.2",
    "lit": "^2.2.2",
    "moment": "^2.29.2",
    "moment-duration-format": "^2.3.2",
    "moment-range": "^4.0.2",
    "parse-duration": "^1.0.2",
    "tinycolor": "^0.0.1",
    "ts-interface-checker": "^1.0.2"
  }
}
